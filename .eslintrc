{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "class-methods-use-this": [
      0
    ],
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "classes/name": [
      "error",
      "class",
      "method"
    ],
    "no-param-reassign": [
      0
    ]
  },
  "plugins": [
    "classes"
  ]
}
